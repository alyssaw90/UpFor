<div class="container">
	<h1>ADMIN SIGNUP</h1>
	<form action="/admin" class="form" method="POST">
		<div class="form-group">
			<label>Venue Name</label>
			<input type="text" class="form-control" name="venueName" placeholder="Enter venue name here">
		</div>
		<div class="form-group">
			<label>First Name</label>
			<input type="text" class="form-control" name="adminFirstName" placeholder="Enter first name here">
		</div>
		<div class="form-group">
			<label>Last Name</label>
			<input type="text" class="form-control"  name="adminLastName" placeholder="Enter last name here">
		</div>
		<div class="form-group">
			<label>Email</label>
			<input type="text" class="form-control"  name="venueEmail" placeholder="Enter email address here">
		</div>
		<div class="form-group">
			<label>Password</label>
			<input type="password" class="form-control" name="password" placeholder="Enter password here">
		</div>
		<div class="form-group">
			<label>Re-type Password</label>
			<input type="password" class="form-control" placeholder="Password confirmation here">
		</div>
		<input type="submit" value="Submit" class="btn btn-primary">
	</form>
</div>