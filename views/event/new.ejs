<div class="container">
	<h1>Add New Event</h1>
	<form action="/event" method="POST">
		<div class="form-group">
			<label for="">Event Name</label>
			<input type="text" class="form-control" placeholder="Event Name Here" name="eventName">
		</div>
		<div class="form-group">
			<label for="">Venue Name</label>
			<input type="text" class="form-control" placeholder="Venue Name Here" name="venueName">
		</div>
		<div class="form-group">
			<label for="">Photo</label>
			<input type="text" class="form-control" placeholder="Link to Photo Here" name="photo">
		</div>
		<div class="form-group">
			<label for="">Price</label>
			<input type="text" class="form-control" placeholder="Price Here (i.e. $10 per ticket)" name="price">
		</div>
		<div class="form-group">
			<label for="">Address</label>
			<input type="text" class="form-control" placeholder="Address Here" name="address">
		</div>
		<div class="form-group">
			<label for="">Event Description</label>
			<textarea name="" id="" cols="30" rows="10" class="form-control" name="eventDescription"></textarea>
		</div>
		<div class="form-group">
			<label for="">Venue Description</label>
			<textarea name="" id="" cols="30" rows="10" class="form-control" name="eventDescription"></textarea>
		</div>
		<div class="form-group">
			<label for="">Link to Ticket</label>
			<input type="text" class="form-control" placeholder="Link to Ticket Here" name="ticketLink">
		</div>
		<div class="form-group">
			<label for="">Start Time</label>
			<input type="text" class="form-control" placeholder="Start Time Here" name="startTime">
		</div>
		<div class="form-group">
			<label for="">End Time</label>
			<input type="text" class="form-control" placeholder="End Time Here" name="endTime">
		</div>
		<div class="form-group">
			<label for="">Start Date</label>
			<input type="text" class="form-control" placeholder="Start DateHere" name="startDate">
		</div>
		<div class="form-group">
			<label for="">End Date</label>
			<input type="text" class="form-control" placeholder="End Date Here" name="endDate">
		</div>
		<div class="form-group">
			<label for="">Interest</label>
			<select name="interest" id="interest" placeholder="Please select an interest">
				<option>Shows & Concerts</option>
				<option>Fine Arts & Theater</option>
				<option>Bars & Nightlife</option>
				<option>Outdoor Adventures</option>
				<option>Sight Seeing</options>
				<option>Sporting Events</option>
				<option>Gaming & Arcades</option>
				<option>Classes</option>
				<option>Community Events</option>
				<option>Festivals</option>
				<option>Bargain Hunter</option>
			</select>
		</div>
		<div class="form-group">
			<label for="">Seattle Neighborhood</label>
			<input type="text" class="form-control" placeholder="Seattle Neighborhood Here" name="neighborhood">
		</div>
		<div class="form-group">
			<label for="">City</label>
			<input type="text" class="form-control" placeholder="City Here" name="city">
		</div>
		<input type="submit" class="btn btn-primary">
	</form>
</div>