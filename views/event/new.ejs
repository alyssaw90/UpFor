<div class="container">
	<h1>Add New Event</h1>
	<form action="">
		<div class="form-group">
			<label for="">Event Name</label>
			<input type="text" class="form-control" placeholder="Event Name Here">
		</div>
		<div class="form-group">
			<label for="">Venue Name</label>
			<input type="text" class="form-control" placeholder="Venue Name Here">
		</div>
		<div class="form-group">
			<label for="">Photo</label>
			<input type="text" class="form-control" placeholder="Link to Photo Here">
		</div>
		<div class="form-group">
			<label for="">Price</label>
			<input type="text" class="form-control" placeholder="Price Here (i.e. $10 per ticket)">
		</div>
		<div class="form-group">
			<label for="">Address</label>
			<input type="text" class="form-control" placeholder="Address Here">
		</div>
		<div class="form-group">
			<label for="">Event Description</label>
			<textarea name="" id="" cols="30" rows="10" class="form-control"></textarea>
		</div>
		<div class="form-group">
			<label for="">Venue Description</label>
			<textarea name="" id="" cols="30" rows="10" class="form-control"></textarea>
		</div>
		<div class="form-group">
			<label for="">Link to Ticket</label>
			<input type="text" class="form-control" placeholder="Link to Ticket Here">
		</div>
		<div class="form-group">
			<label for="">Start Time</label>
			<input type="text" class="form-control" placeholder="Start Time Here">
		</div>
		<div class="form-group">
			<label for="">End Time</label>
			<input type="text" class="form-control" placeholder="End Time Here">
		</div>
		<div class="form-group">
			<label for="">Start Date</label>
			<input type="text" class="form-control" placeholder="Start DateHere">
		</div>
		<div class="form-group">
			<label for="">End Date</label>
			<input type="text" class="form-control" placeholder="End Date Here">
		</div>
		<div class="form-group">
			<label for="">Interest</label>
			<input type="text" class="form-control" placeholder="Choose Interest">
		</div>
		<div class="form-group">
			<label for="">Seattle Neighborhood</label>
			<input type="text" class="form-control" placeholder="Seattle Neighborhood Here">
		</div>
		<div class="form-group">
			<label for="">City</label>
			<input type="text" class="form-control" placeholder="City Here">
		</div>
		<input type="submit" class="btn btn-primary">
	</form>
</div>